msgstr ""

#: ../../advanced.rst:272
# 8884fc2110784136b6995e7324da5de1
msgid "**Figure 5.2b: Set the Root Password**"
msgstr ""

#: ../../advanced.rst:276
# f68ff545ced142c986eb2fe9d60efdc7
msgid "Input and confirm the root password then click \"Next\" to proceed to the screen shown in Figure 5.2c."
msgstr ""

#: ../../advanced.rst:278
# 6d03e31b7c414658a6aa70482e748853
msgid "**Figure 5.2c: Create the Primary User Account**"
msgstr ""

#: ../../advanced.rst:282
# 7c2d4100d92e4309ac131c904cf8d77c
