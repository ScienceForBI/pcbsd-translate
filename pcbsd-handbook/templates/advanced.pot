msgstr ""

#: ../../advanced.rst:288
# 37273dc3ac7c47e496d33d8142be39b5
msgid "**Name:** can contain capital letters and spaces."
msgstr ""

#: ../../advanced.rst:290
# 06842cad5aea4bb5bf64cf702718f91c
msgid "**Username:** the name used when logging in. Can not contain spaces and is case sensitive (e.g. *Kris* is a different username than *kris*)."
msgstr ""

#: ../../advanced.rst:292
# 7a9f5eb96a0c4bc2a29572e27dbd715b
msgid "**Password:** the password used when logging in. You must type it twice in order to confirm it."
msgstr ""

#: ../../advanced.rst:294
# 8aecc0d116c34df8a85d74fa668e03a0
msgid "**Default shell:** use the drop-down menu to select the **csh**, **tcsh**, **sh**, or **bash** login shell."
msgstr ""

#: ../../advanced.rst:296
# 7b82516a12684f66b78dd0720c263fa2
msgid "When finished, click \"Next\" to proceed to the screen shown in Figure 5.2d."
msgstr ""

